<h1>New Process</h1>


<%= form_tag(processes_url, method: 'post') do %>


  <div>
    <%= label_tag :name %><br />
    <%= text_field_tag :name %>
  </div>

    <div>
    <%= label_tag :start_date %><br />
    <%= date_select_tag :start_date %>
  </div>

  
  <div>
    <%= submit_tag "Create Process" %>
  </div>
<% end %>
