<h1>New Form</h1>


<%= form_tag(forms_url, method: 'post') do %>


  <div>
    <%= label_tag :source %><br />
    <%= text_field_tag :source %>
  </div>

    <div>
    <%= label_tag :name %><br />
    <%= text_field_tag :name %>
  </div>

    <div>
    <%= label_tag :process_id %><br />
    <%= number_field_tag :process_id %>
  </div>

  
  <div>
    <%= submit_tag "Create Form" %>
  </div>
<% end %>
